<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="400.0" prefWidth="600.0"
            xmlns="http://javafx.com/javafx/8.0.171" fx:controller="Controller.StartController">
    <children>
        <Button layoutX="35.0" layoutY="311.0" mnemonicParsing="false" onAction="#loadOnline" text="Load Online Data"/>
        <Button fx:id="btHolder" layoutX="35.0" layoutY="256.0" mnemonicParsing="false" onAction="#loadLocal"
                text="Load Local Data"/>
        <ImageView fx:id="indLoc" fitHeight="55.0" fitWidth="57.0" layoutX="529.0" layoutY="331.0" pickOnBounds="true"
                   preserveRatio="true"/>
    </children>
</AnchorPane>
